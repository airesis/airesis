.timeline {
    background-color: silver;
    height: 5px;
    margin-top: 18px;
    margin-bottom: 5px;
    font-weight: bold;
    font-size: 12px;
    border-radius: 10px;
    box-shadow: 1px 1px 2px silver;
    position: relative;
    opacity: 0.7;
    margin-left: 10px;
    margin-right: 15px;

    .cur {
        height: 13px;
        width: 13px;
        border-radius: 50%;
        position: absolute;
        cursor: help;
        box-shadow: 0px 0px 1px #000;
        margin-top: -4px;
        background-color: #e6e6e6;
        z-index: 1;
        margin-left: -6px;
        opacity: 0.9;

        &.stop {
            left: 100%;
            background: #e6e6e6 url(/img/icon/prop/clock.png) no-repeat center;
            background-size: 18px;
        }

        &.now {
            left: 100%;
        }

        &.quorum {
            margin-top: -8px;
            width: 0;
            height: 0;
            border-left: 12px solid transparent;
            border-right: 12px solid transparent;
            border-top: 15px solid #767676;
            box-shadow: none;
            background-color: transparent;
            margin-left: -13px;
            &.bad {
                margin-top: 0px;
                border-top: none;
                border-bottom: 15px solid #767676;
            }
        }

    }

    .time {
        background-color: red;
        height: 100%;
        width: 0%;
        border-top-left-radius: 10px;
        border-bottom-left-radius: 10px;
        position: relative;
        .good {
            background-color: #07c100;
            height: 100%;
            width: 0%;
            margin-left: -1px;
            float: left;
            border-top-left-radius: 10px;
            border-bottom-left-radius: 10px;
        }

    }

    &.vote {
        .time {
            background-color: white;
            border: 1px solid silver;
            .good {
                background-color: blue;
            }
        }
    }
}


