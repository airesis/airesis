$("#rankingpanelcontainer<%=params[:id]%>").html('<%=j (render :partial => 'proposal_comments/ranking_panel', :locals => {:comment => @proposal_comment})%>');
<%if @proposal_comment.paragraph_id %>
    var data_section_id = "[data-section_id=<%=@proposal_comment.paragraph.section.id%>]";
    var contribute_button = $('.contributeButton'+data_section_id);
    var new_contributes = parseInt(contribute_button.attr('data-unread_contributes_num'));
    contribute_button.attr('data-unread_contributes_num',new_contributes-1);

<%end%>